{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "nofacefrontend",
  "version": "0.1.0",
  "identifier": "com.squalltcg.nofacefrontend",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "No Face Client",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "@tauri-apps/plugin-dialog": {},
    "@tauri-apps/plugin-fs": {}
  }
}
